{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "no-useless-return": 0,
        "no-alert": 0,
        "no-new": 0,
        "max-len": 0,
        "arrow-body-style": 0,
        "quote-props": 0,
        "no-undef": 0
    }
}
